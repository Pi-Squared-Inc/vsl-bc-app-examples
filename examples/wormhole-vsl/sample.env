INBOUND_LIMIT=1000
TRANSFER_AMOUNT=100

# Source chain config
# The source chain Wormhole chain ID can be found at https://wormhole.com/docs/build/reference/chain-ids/. For example, the Ethereum Sepolia chain Wormhole chain ID is 10002. We use 1 for local deployment.
SRC_CHAIN_ID=<Source Chain Wormhole ID> # e.g. 10002
# The source chain RPC URL, it is recommended to create one using https://tenderly.co.
SRC_RPC_URL=<Source Chain RPC URL> # e.g. http://localhost:8545
# This is optional, you can create one on the corresponding chain's browser. For example, for the Ethereum Sepolia chain, you can create it at https://etherscan.io/myapikey.
SRC_ETHERSCAN_API_KEY=
# The private key of the account you are going to use to deploy the contract. If the private key of your account doesn't have the "0x" prefix, please add it.
SRC_PRIVATE_KEY=<Source Chain Private Key> # e.g. 0xac0974bec39a17e36ba4a6b4d238ff944bacb478cbed5efcae784d7bf4f2ff80

# The following values are generated by the deploy source chain step, 
SRC_TOKEN_ADDRESS=<Source Token Contract Address> # e.g. 0x9fE46736679d2D9a65F0992F2272dE9f3c7fa6e0
SRC_VSL_ADDRESS=<Source VSL Contract Address> # e.g. 0xCf7Ed3AccA5a467e9e704C703E8D87F634fB0Fc9
SRC_MANAGER_ADDRESS=<Source NttManager Contract Address> # e.g. 0x5FC8d32690cc91D4c39d9d3abcBD16989F875707
SRC_TRANSCEIVER_ADDRESS=<Source Transceiver Contract Address> # e.g. 0x2279B7A0a67DB372996a5FaB50D91eAA73d2eBe6

# Dest chain config
# The dest chain Wormhole chain ID can be found at https://wormhole.com/docs/build/reference/chain-ids/. For example, the Arbitrum Sepolia chain Wormhole chain ID is 10003. We use 2 for local deployment.
DEST_CHAIN_ID=<Destination Chain Wormhole ID> # e.g. 2
# The dest chain RPC URL, it is recommended to create one using https://tenderly.co.
DEST_RPC_URL=<Destination Chain RPC URL> # e.g. http://localhost:8546
# This is optional, you can create one on the corresponding chain's browser. For example, for the Arbitrum Sepolia chain, you can create it at https://arbiscan.io/myapikey.
DEST_ETHERSCAN_API_KEY=
# The private key of the account you are going to use to deploy the contract. If the private key of your account doesn't have the "0x" prefix, please add it.
DEST_PRIVATE_KEY=<Destination Chain Private Key> # e.g. 0xac0974bec39a17e36ba4a6b4d238ff944bacb478cbed5efcae784d7bf4f2ff80

# The following values are generated by the deploy destination chain step,
DEST_TOKEN_ADDRESS=<Destination Chain Token Contract Address> # e.g. 0x9fE46736679d2D9a65F0992F2272dE9f3c7fa6e0
DEST_VSL_ADDRESS=<Destination Chain VSL Contract Address> # e.g. 0xCf7Ed3AccA5a467e9e704C703E8D87F634fB0Fc9
DEST_MANAGER_ADDRESS=<Destination Chain NttManager Contract Address> # e.g. 0x5FC8d32690cc91D4c39d9d3abcBD16989F875707
DEST_TRANSCEIVER_ADDRESS=<Destination Chain Transceiver Contract Address> # e.g. 0x2279B7A0a67DB372996a5FaB50D91eAA73d2eBe6
