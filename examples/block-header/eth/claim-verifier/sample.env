# Rust logging level
RUST_LOG=<Log Level> # info
# The VSL RPC URL
VSL_URL=<VSL RPC URL> # https://rpc.vsl.pi2.network
# Backend service URL
BACKEND_URL=<Backend URL> # http://backend:3001
