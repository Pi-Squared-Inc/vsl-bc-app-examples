VERIFIER_PRIVATE_KEY=<Verifier Private Key> # f3c39a64164703f176158cd6f947cb77e18ef8863445564d110a8d64014cc81a
# Public key for header signing
HEADER_SIGNER_PUBLIC_KEY=<Header Signer Public Key> # 0x037b0aa81a2c6f63da0be46162433e5afeb7668bccb89bb401f1199bafc7de6fa6
